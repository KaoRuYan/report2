      "source": [
        "import math\n",
        "s=input(\"請輸入你要求的一元二次方程的三個參數a、b、c,用逗號隔開\")\n",
        "a,b,c=s.split(',')\n",
        "a=int(a)\n",
        "b=int(b)\n",
        "c=int(c)\n",
        "if a==0:\n",
        "    print(-c/b)\n",
        "elif b*b-4*a*c<0:\n",
        "    print(\"沒有結果\")\n",
        "elif b*b-4*a*c==0:\n",
        "    print(-b/(2*a))\n",
        "else:\n",
        "    print((-b+math.sqrt(b*b-4*a*c))/(2*a),(b-math.sqrt(b*b-4*a*c)/(-2*a)))"
      ]
    }
